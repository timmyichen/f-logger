  "bugs": {
    "url": "https://github.com/timmyichen/f-logger/issues"
  },
  "homepage": "https://github.com/timmyichen/f-logger#readme",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "consolidate": "^0.15.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "mongoose": "^5.2.9",
    "mongoose-unique-validator": "^2.0.1",
    "morgan": "^1.9.0",
    "nunjucks": "^3.1.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
